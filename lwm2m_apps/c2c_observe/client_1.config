# security credentials to be used with the server
CONFIG_PSK_ID="Client_Identity"
CONFIG_PSK_KEY="ThisIsRIOT!"

# Client 1 - observer
CONFIG_CLIENT_TYPE_OBSERVER=y

# This client configuration, also used as endpointName
CONFIG_LWM2M_DEVICE_NAME="riot:client:0001"

# URI of client 2
CONFIG_LWM2M_CLIENT_URI="coap://[fe80::2491:56ff:fe33:4e45]" # port inferred from security mode when undefined
# endpoint name of client 2
CONFIG_LWM2M_CLIENT_ENDPOINT="riot:client:0002"
